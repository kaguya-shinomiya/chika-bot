{
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-graphql-request": "^3.2.1",
    "@graphql-codegen/typescript-operations": "^1.18.0",
    "@types/dotenv-safe": "^8.1.1",
    "@types/ffmpeg-static": "^3.0.0",
    "@types/he": "^1.1.1",
    "@types/ioredis": "^4.26.4",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.3.0",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-mock-extended": "^1.0.15",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "prisma": "^2.24.1",
    "ts-jest": "^27.0.3",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.0",
    "@prisma/client": "^2.24.1",
    "axios": "^0.21.1",
    "discord.js": "^12.5.3",
    "dotenv-cli": "^4.0.0",
    "ffmpeg-static": "^4.3.0",
    "graphql": "^15.5.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.12.4",
    "he": "^1.2.0",
    "ioredis": "^4.27.5",
    "lodash": "^4.17.21",
    "pm2": "^5.1.0",
    "ytdl-core": "^4.8.2",
    "ytpl": "^2.2.1",
    "ytsr": "^3.5.0"
  },
  "resolutions": {
    "normalize-url": "^4.5.1",
    "ws": "^7.4.6"
  },
  "scripts": {
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "build": "tsc && prisma migrate deploy",
    "docker:up": "docker-compose up -d && yarn db:push",
    "docker:down": "docker-compose down",
    "watch": "tsc -w",
    "dev:bot": "PROCESS_TYPE=bot dotenv -e .env.development -- nodemon dist/index.js",
    "dev:web": "PROCESS_TYPE=web dotenv -e .env.development -- nodemon dist/index.js",
    "test": "dotenv -e .env.test -- jest --watch -i",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "db:push": "dotenv -e .env.development -- prisma db push",
    "db:migrate": "dotenv -e .env.development -- prisma migrate dev",
    "db:edit": "dotenv -e .env.development -- prisma studio",
    "gen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "yarn eslint",
      "yarn prettier --write"
    ]
  },
  "engines": {
    "node": "14.17.1",
    "yarn": "1.22.10"
  },
  "license": "MIT",
  "version": "0.3.4"
}
